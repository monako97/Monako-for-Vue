 {
     "presets": [
       [
         "@babel/preset-env",
         {
           "useBuiltIns": "usage",
           "modules": false,
           "corejs": "2",
           "targets": {
             "browsers": "last 2 versions, not ie <= 9"
           }
         }
       ]
     ],
     "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "helpers": false
            }
        ]
     ]
 }
